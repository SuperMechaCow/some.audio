  "devDependencies": {
    "autoprefixer": "9.7.1",
    "babel-eslint": "10.0.3",
    "babel-minify": "0.5.1",
    "cssnano": "4.1.10",
    "postcss-cli-simple": "3.0.0",
    "sortpack": "2.0.4",
    "standard": "14.3.1"
  },
  "funding": {
    "type": "ko-fi",
    "url": "https://ko-fi.com/zacanger"
  }
