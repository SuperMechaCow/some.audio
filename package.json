{
  "name": "some.audio",
  "description": "Simple, anonymous audio file host",
  "version": "0.8.1",
  "private": true,
  "author": {
    "email": "zac@zacanger.com",
    "name": "Zac Anger",
    "url": "http://zacanger.com"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "build": "./scripts/build-css.sh && ./scripts/build-js.sh",
    "prd": "NODE_ENV=production PORT=80 forever start -a -l f.log -e e.log -o o.log lib/index.js",
    "prebuild": "npm t && sortpack",
    "preversion": "npm run build",
    "start": "./scripts/start.sh",
    "start:serve": "nodemon lib/index.js",
    "test": "npm run test:lint",
    "test:lint": "standard"
  },
  "homepage": "https://some.audio",
  "repository": {
    "type": "git",
    "url": "https://github.com/zacanger/some.audio"
  },
  "bugs": "https://github.com/zacanger/some.audio/issues",
  "keywords": [
    "audio",
    "host",
    "mp3",
    "music"
  ],
  "dependencies": {
    "boring-cluster": "5.0.2",
    "compression": "1.7.4",
    "dateformat": "3.0.3",
    "express": "4.16.4",
    "express-busboy": "7.0.1",
    "express-pino-logger": "4.0.0",
    "express-static-gzip": "1.1.3",
    "express-winston": "3.1.0",
    "forever": "1.0.0",
    "glob": "7.1.4",
    "helmet": "3.18.0",
    "mmmagic": "0.5.3",
    "monk": "7.0.0",
    "music-metadata": "3.8.0",
    "nodemon": "1.19.0",
    "pino": "5.12.4",
    "sanitize-filename": "1.6.1",
    "winston": "3.2.1",
    "zeelib": "8.6.0"
  },
  "devDependencies": {
    "autoprefixer": "9.5.1",
    "babel-eslint": "10.0.1",
    "babel-minify": "0.5.0",
    "cssnano": "4.1.10",
    "postcss-cli-simple": "3.0.0",
    "sortpack": "1.1.6",
    "standard": "12.0.1"
  }
}
