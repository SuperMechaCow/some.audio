{
  "name": "some.audio",
  "description": "Simple, anonymous audio file host",
  "version": "0.6.2",
  "private": true,
  "author": {
    "email": "zac@zacanger.com",
    "name": "Zac Anger",
    "url": "http://zacanger.com"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "build": "./scripts/build-css.sh && ./scripts/build-js.sh",
    "prd": "NODE_ENV=production PORT=80 forever start -a -l f.log -e e.log -o o.log lib/index.js",
    "prebuild": "npm t && sortpack",
    "preversion": "npm run build",
    "start": "./scripts/start.sh",
    "start:serve": "nodemon lib/index.js",
    "test": "npm run test:lint",
    "test:lint": "eslint -c .eslintrc.json lib"
  },
  "homepage": "https://some.audio",
  "repository": {
    "type": "git",
    "url": "https://github.com/zacanger/some.audio"
  },
  "bugs": "https://github.com/zacanger/some.audio/issues",
  "keywords": [
    "audio",
    "host",
    "mp3",
    "music"
  ],
  "dependencies": {
    "boring-cluster": "5.0.1",
    "compression": "1.7.4",
    "dateformat": "3.0.3",
    "express": "4.16.4",
    "express-busboy": "7.0.1",
    "express-static-gzip": "1.1.3",
    "forever": "1.0.0",
    "glob": "7.1.3",
    "helmet": "3.16.0",
    "mmmagic": "0.5.2",
    "monk": "6.0.6",
    "music-metadata": "3.6.1",
    "nodemon": "1.18.11",
    "sanitize-filename": "1.6.1",
    "zeelib": "8.6.0"
  },
  "devDependencies": {
    "autoprefixer": "9.5.1",
    "babel-eslint": "10.0.1",
    "babel-minify": "0.5.0",
    "cssnano": "4.1.10",
    "eslint": "5.16.0",
    "eslint-config-zacanger": "3.4.2",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-unicorn": "8.0.2",
    "postcss-cli-simple": "3.0.0",
    "sortpack": "1.1.5"
  }
}
